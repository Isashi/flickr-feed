<div class="flickr">
  <ul>
    <% photos.each do |row| %>
      <li>
        <% row.each do |p| %>
        	<% if p %>
          <%= link_to(image_tag(FlickRaw.url_q(p), :class => 'flickr_photo', :title => p.title, :border => 0, :size => '150x150'), FlickRaw.url_photopage(p)) %>
          <a href=<%=FlickRaw.url_b(p)%> download><%= image_tag("/assets/download.png") %></a>
          <% end %>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>