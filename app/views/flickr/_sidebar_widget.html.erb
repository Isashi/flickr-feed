<ul class="polaroid-images">
    <% photos.each do |p| %>
    	<% if p %>
            <li class="frame">            
                <%= link_to(image_tag(FlickRaw.url_q(p), :class => 'polaroid-images', :title => p.title, :border => 0, :size => '170x170'), 
                FlickRaw.url_photopage(p), :title =>  (title(p.title))) %>
            </li>
        <% end %>
    <% end %>
</ul>